{
    "manifest_version" : 3,
    "name" : "취준의 모든 정보",
    "description" : "힘든 취업준비 기업 정보라도 쉽게 얻자",
    "version" : "1.0",
    "permissions": [
    "activeTab",
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "run_at": "document_end"
        }
    ]

}